<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/main"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context=".LevelDashboard">

    <Button
        android:id="@+id/buttonGoBackLevelDB"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginTop="16dp"
        android:text="Go Back"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.051"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent" />

    <Button
        android:id="@+id/buttonLevel1DB"
        android:layout_width="378dp"
        android:layout_height="53dp"
        android:layout_marginTop="60dp"
        android:text="Level 1"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.515"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/buttonGoBackLevelDB" />

    <Button
        android:id="@+id/buttonLevel2DB"
        android:layout_width="378dp"
        android:layout_height="53dp"
        android:layout_marginTop="20dp"
        android:text="Level 2"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.484"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/buttonLevel1DB" />

    <Button
        android:id="@+id/buttonLevel3DB"
        android:layout_width="378dp"
        android:layout_height="53dp"
        android:layout_marginTop="20dp"
        android:text="Level 3"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.484"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/buttonLevel2DB" />

    <Button
        android:id="@+id/buttonLevel4DB"
        android:layout_width="378dp"
        android:layout_height="53dp"
        android:layout_marginTop="24dp"
        android:text="Level 4"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.515"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/buttonLevel3DB" />

    <Button
        android:id="@+id/buttonLevel5DB"
        android:layout_width="378dp"
        android:layout_height="53dp"
        android:layout_marginTop="24dp"
        android:text="Level 5"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.515"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/buttonLevel4DB" />

    <Button
        android:id="@+id/buttonLevel6DB2"
        android:layout_width="378dp"
        android:layout_height="53dp"
        android:layout_marginTop="20dp"
        android:text="Level 6"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.484"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/buttonLevel5DB" />

    <Button
        android:id="@+id/buttonLevel7DB"
        android:layout_width="378dp"
        android:layout_height="53dp"
        android:layout_marginTop="12dp"
        android:text="Level 7"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.484"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/buttonLevel6DB2" />

    <Button
        android:id="@+id/buttonLevel8DB2"
        android:layout_width="378dp"
        android:layout_height="53dp"
        android:text="Level 8"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.484"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/buttonLevel7DB"
        app:layout_constraintVertical_bias="0.29" />

    <TextView
        android:id="@+id/textViewLevelSB"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginStart="16dp"
        android:text="Levels:"
        android:textSize="25dp"
        app:layout_constraintBottom_toTopOf="@+id/buttonLevel1DB"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/buttonGoBackLevelDB"
        app:layout_constraintVertical_bias="0.2" />

    <Button
        android:id="@+id/buttonRandomize"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginTop="16dp"
        android:text="Random"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.849"
        app:layout_constraintStart_toEndOf="@+id/buttonGoBackLevelDB"
        app:layout_constraintTop_toTopOf="parent" />
</androidx.constraintlayout.widget.ConstraintLayout>